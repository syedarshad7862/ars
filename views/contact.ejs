<%- include('layouts/header')%>
<!-- contact form  -->
<div class="form">
    <div class="container">
        <form action="/contact" method="post">
            <h1 class="form-title">Contact Me</h1>
                <label for="name" class="form-label">Name</label>
                <input type="text" id="name" autofocus class="form-input" name="name" required>
            
                <label for="email" class="form-label">Email Address</label>
                <input type="email" id="email" autofocus class="form-input" name="email"
                    required>
                <label for="subject" class="form-label">Subject</label>
                <input type="text" id="subject" autofocus class="form-input" name="subject"
                    required>
                <textarea name="message" class="form-input" id="textarea" cols="30" rows="10"
                    placeholder=" Write Message Here..." required></textarea>
            <input type="submit" class="input-btn" id="btn" value="Submit">
        </form>
        <!-- map start -->
        <div class="map">
            <iframe
                src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d3808.592282941551!2d78.45022287433963!3d17.33521020430783!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x3bcb97de2fb48c73%3A0xca05cb457e7d9c9d!2sCity%20Plaza%20Function%20Hall!5e0!3m2!1sen!2sin!4v1689167791816!5m2!1sen!2sin"
                style="border:0;" allowfullscreen="" loading="lazy"
                referrerpolicy="no-referrer-when-downgrade"></iframe>
        </div>
        <!-- map end -->
    </div>
</div>    
    <!-- contact form end -->
<%- include('layouts/footer')%>